{
  "name": "mymasterbook-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:DEV": "ng serve --configuration development --port=4200",
    "start:LOC": "ng serve --configuration local --port=4200",
    "start:STG": "ng serve --configuration staging --port=4200",
    "start:PRD": "ng serve --configuration production --port=4200",
    "build": "ng build",
    "build:DEV": "ng build --configuration development",
    "build:LOC": "ng build --configuration local",
    "build:STG": "ng build --configuration staging",
    "build:PRD": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "watch:DEV": "ng build --watch --configuration development",
    "watch:LOC": "ng build --watch --configuration local",
    "watch:STG": "ng build --watch --configuration staging",
    "watch:PRD": "ng build --watch --configuration production",
    "ssr-build": "ng build && ng run mymasterbook-portal:server",
    "ssr-build:DEV": "ng build --configuration development && ng run mymasterbook-portal:server:development",
    "ssr-build:LOC": "ng build --configuration local && ng run mymasterbook-portal:server:local",
    "ssr-build:STG": "ng build --configuration staging && ng run mymasterbook-portal:server:staging",
    "ssr-build:PRD": "ng build --configuration production && ng run mymasterbook-portal:server:production",
    "ssr-serve": "node dist/mymasterbook-portal/server/main.js",
    "ssr-start": "ng run mymasterbook-portal:serve-ssr",
    "ssr-start:DEV": "ng run mymasterbook-portal:serve-ssr:development",
    "ssr-start:LOC": "ng run mymasterbook-portal:serve-ssr:local",
    "ssr-start:STG": "ng run mymasterbook-portal:serve-ssr:staging",
    "ssr-start:PRD": "ng run mymasterbook-portal:serve-ssr:production",
    "prerender": "ng run mymasterbook-portal:prerender",
    "build-prod": "ng build --configuration production --stats-json --aot=true --build-optimizer",
    "build-to-deploy:ssr": "npm run build-prod && ng run mymasterbook-portal:server:production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "private": true,
